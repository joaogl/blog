{
    "name": "jlourenco/blog",
    "type": "library",
    "description": "This module adds a ready to use blog to your website.",
    "version": "0.1.3.6",
    "keywords": [
        "jlourenco",
        "blog"
    ],
    "homepage": "https://github.com/jlourenco/blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Lourenco",
            "email": "jglourenco.pt@gmail.com",
            "homepage": "http://joaolourenco.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "jlourenco/base" : "0.1.*",
        "jlourenco/comments" : "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jlourenco\\blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jlourenco\\blog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
